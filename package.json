{
  "name": "seneca-transport-test",
  "version": "0.2.0",
  "description": "Standard test cases for Seneca transports.",
  "main": "transport-test.js",
  "scripts": {
    "test": "lab -dL -I WebAssembly transport-test.js",
    "coveralls": "lab -s -P test -r lcov -I WebAssembly | coveralls",
    "coverage": "lab -v -P test -L -t 80 -r -I WebAssembly html > docs/coverage.html",
    "annotate": "docco transport.js -o docs/annotated",
    "prettier": "prettier --write --no-semi --single-quote transport-test.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/senecajs/seneca-transport-test.git"
  },
  "keywords": [
    "seneca",
    "transport",
    "test"
  ],
  "author": "Richard Rodger (http://richardrodger.com)",
  "contributors": [
    "Marco Piraccini (https://github.com/marcopiraccini)"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/senecajs/seneca-transport-test/issues"
  },
  "homepage": "https://github.com/senecajs/seneca-transport-test",
  "devDependencies": {
    "docco": "0.7.x",
    "lab": "11.0.x",
    "pre-commit": "1.1.3",
    "prettier": "^1.9.2"
  },
  "files": [
    "LICENSE",
    "README.md",
    "transport-test.js"
  ]
}
